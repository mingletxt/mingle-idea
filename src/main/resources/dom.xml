<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <pluginGroups></pluginGroups>
    <proxies></proxies>
    <mirrors>
        <mirror>
            <id>webank</id>
            <mirrorOf>*</mirrorOf>
            <name>webank</name>
            <url>http://maven.weoa.com/nexus/content/groups/public/</url>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>webank</id>
            <username>deployment</username>
            <password>deployment123</password>
        </server>
        <server>
            <id>Releases</id>
            <username>classified</username>
            <password>classified</password>
        </server>
        <server>
            <id>Snapshots</id>
            <username>classified</username>
            <password>classified</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <id>webank</id>
            <repositories>
                <repository>
                    <id>Releases</id>
                    <name>Releases</name>
                    <url>http://maven.weoa.com/nexus/content/repositories/releases/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                </repository>
                <repository>
                    <id>Snapshots</id>
                    <name>Snapshots</name>
                    <url>http://maven.weoa.com/nexus/content/repositories/snapshots/</url>
                    <snapshots>
                        <updatePolicy>always</updatePolicy>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
</settings>